    "function rotation(v, teta)\r\n",
    "    rotmatrix=[cos(teta) -sin(teta); sin(teta) cos(teta)]\r\n",
    "    vprime=v*rotmatrix\r\n",
    "    return vprime\r\n",
    "end"
   ],
